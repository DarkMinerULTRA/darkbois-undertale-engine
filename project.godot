; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="darkboi's UNDERTALE engine"
run/main_scene="res://rooms/room_intro.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="darkboiEngine"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=30
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://splash.png"
boot_splash/use_filter=false
config/icon="res://assets/sprites/battle/spr_heart_1.png"

[autoload]

global="*res://scripts/global.gd"
ui_quit="*res://rooms/ui_quit.tscn"
dialogueHandler="*res://objects/dialogue.tscn"
battleHandler="*res://scripts/battleHandler.gd"
playerVariables="*res://scripts/player_variables.gd"
data="*res://scripts/data.gd"
fade="*res://objects/obj_fade.tscn"
BGM="*res://scripts/bgm.gd"
time_data="*res://scripts/time.gd"
ui_save="*res://objects/ui_save.tscn"

[display]

window/size/viewport_width=320
window/size/viewport_height=240
window/size/resizable=false
window/size/window_width_override=640
window/size/window_height_override=480
window/stretch/mode="canvas_items"

[gui]

theme/custom_font="res://assets/fonts/DTM-Mono.otf"
theme/default_font_antialiasing=0

